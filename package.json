{
  "name": "ts-jest-test-case",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "babel-jest": "^22.4.4",
    "babel-preset-env": "^1.7.0",
    "jest": "^22.4.4",
    "ts-jest": "^22.4.6"
  }
}
